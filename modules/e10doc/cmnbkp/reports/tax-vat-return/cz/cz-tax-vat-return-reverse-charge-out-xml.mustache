{{xmlMode}}<?xml version="1.0" encoding="UTF-8"?>

<Pisemnost nazevSW="Shipard" verzeSW="2014.1.0">

	<DPHEVD verzePis="01.02">

		<VetaD
			k_uladis="DPH"
			dokument="EVD"
			typ_vypisu="D"
{{#data.taxVatReturn.taxPeriod.month}}

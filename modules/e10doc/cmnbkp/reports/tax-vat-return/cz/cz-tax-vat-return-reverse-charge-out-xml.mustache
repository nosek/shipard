{{/data.taxVatReturn.e10-CZ-VR-zast.e10-CZ-VR-typZastupce}}
		/>

{{#data.taxVatReturn.reverseChargeOut.pages}}
{{#data}}
{{#base}}
		<VetaE
			c_radku="{{xmlLineNumber}}"
			dic_dod="{{shortpvin}}"
			kod_pred_pl="{{code}}"
			d_uskut_pl="{{dateTax}}"
			zakl_dane="{{base}}"
{{#amount}}
			roz_pl="{{amount}}"
			roz_pl_j="{{unitText}}"
{{/amount}}
		/>
{{/base}}
{{/data}}
{{/data.taxVatReturn.reverseChargeOut.pages}}

	</DPHEVD>

</Pisemnost>

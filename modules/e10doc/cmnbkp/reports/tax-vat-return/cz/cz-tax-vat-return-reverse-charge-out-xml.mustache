			rok="{{data.taxVatReturn.taxPeriod.year}}"
{{/data.taxVatReturn.taxPeriod.quarter}}
{{#data.taxVatReturn.taxPeriod.period.start}}
			zdobd_od="{{data.taxVatReturn.taxPeriod.period.start}}"
{{/data.taxVatReturn.taxPeriod.period.start}}
{{#data.taxVatReturn.taxPeriod.period.end}}
			zdobd_do="{{data.taxVatReturn.taxPeriod.period.end}}"
{{/data.taxVatReturn.taxPeriod.period.end}}
			d_poddp="{{data.head.dateIssue}}"
		/>

		<VetaP
			c_ufo="{{data.taxVatReturn.e10-CZ-VR.e10-CZ-VR-FU}}"
			c_pracufo="{{data.taxVatReturn.e10-CZ-VR.e10-CZ-VR-pracFU}}"
			typ_ds="{{data.taxVatReturn.e10-CZ-VR-subjekt.e10-CZ-VR-typSubjektu}}"

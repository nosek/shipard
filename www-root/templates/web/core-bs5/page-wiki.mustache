                    {{/serverInfo.images.template.headerLogo}}
					{{#serverInfo.templateParams.headerTitle}}<span>{{serverInfo.templateParams.headerTitle}}</span>{{/serverInfo.templateParams.headerTitle}}
				</a>
			</span>
		<button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>

		<div class="collapse navbar-collapse" id="navbarCollapse">
			{{#data.sections.0}}
				<ul class="nav navbar-nav">
					{{#data.sections}}
						{{#topMenu}}
							<li class='nav-item{{#active}} active{{/active}}'{{^topMenuText}} title='{{title}}'{{/topMenuText}}>
								<a class="nav-link" href="{{urlRoot}}/{{id}}">{{#icon}}<i class="{{@icon;dataItem:icon}}"></i>{{/icon}}<span{{^topMenuText}} class="d-lg-none"{{/topMenuText}}> {{title}}</span></a>
							</li>
						{{/topMenu}}
					{{/data.sections}}
				</ul>
			{{/data.sections.0}}


			<ul class="nav navbar-nav nav-flex-icons ml-auto">
				<li class="nav-item{{#page.url_q}} active{{/page.url_q}}"><a href="{{urlRoot}}/q" class="nav-link"><i class="fa fa-search"></i> <span class="XXXhidden-sm-down">Hledání</span></a></li>
				{{#userAuthenticated}}
					<li class="nav-item"><span class="nav-link"><i class="fa fa-user-circle-o"></i> {{userName}}</span></li>
					<li class="nav-item"><a class="nav-link " href="{{@urlUser;type:logout}}"  title="Odhlášení"><i class="fa fa-sign-out"></i></a></li>
				{{/userAuthenticated}}
				{{^userAuthenticated}}
					<li class="nav-item"><a class="nav-link " href="{{@urlUser;type:login}}"><i class="fa fa-sign-in"></i> Přihlásit se</a></li>
				{{/userAuthenticated}}
			</ul>
		</div>
	</nav>
	{{^serverInfo.templateParams.topMenuFixedTop}}</div>{{/serverInfo.templateParams.topMenuFixedTop}}
</header>


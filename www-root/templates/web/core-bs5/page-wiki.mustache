    <style type="text/css">
        .sticky {
            position: fixed;
            top: 0;
            padding-top: 3rem;
            width: 100%;
            background-color: white;
            box-shadow: 0 2px 2px rgba(0,0,0,.2);
            font-size: 80%;
        }
        .sticky > ul.breadcrumb {display: none;}
        .sticky h2 {font-size: 120%;}
        .sticky td.e10-wiki-page-header-title {padding-left: 1rem;}
        .sticky td.e10-wiki-page-header-buttons {padding-right: 1rem;}

        /* Add some top padding to the page content to prevent sudden quick movement (as the header gets a new position at the top of the page (position:fixed and top:0) */
        .sticky + .e10-wiki-page {
            /*padding-top: 3rem;*/
        }

    </style>
